{
  "jest.jestCommandLine": "yarn test",
  "jest.autoRun": "off",
  "files.exclude": {
    "p5-analysis": true,
    "p5-server": true
  },
  "json.schemas": [
    {
      "fileMatch": [
        "p5-analysis/**/libraries/*-libraries.json"
      ],
      "url": "./p5-analysis/src/lib/schemas/libraries.json"
    },
    {
      "fileMatch": [
        "p5-analysis/**/libraries/categories.json"
      ],
      "url": "./p5-analysis/src/lib/schemas/categories.json"
    }
  ]
}
