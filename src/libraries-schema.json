{
  "$schema": "https://json-schema.org/draft/2020-12/schema",
  "$id": "https://example.com/product.schema.json",
  "title": "Libraries",
  "description": "A list of p5.js libraries",
  "type": "array",
  "items": {
    "type": "object",
    "uniqueItems": true,
    "properties": {
      "name": {
        "description": "The human-readable name of the library",
        "type": "string"
      },
      "homepage": {
        "description": "The library's home page",
        "type": "string"
      },
      "path": {
        "description": "A CDN URL that loads the library",
        "type": "string"
      },
      "packageName": {
        "description": "NPM package name",
        "type": "string"
      },
      "globals": {
        "description": "Global variables defined by the library",
        "type": "array",
        "items": {
          "type": "string"
        }
      },
      "props": {
        "description": "p5 properties added by the library",
        "type": "array",
        "items": {
          "type": "string"
        }
      }
    },
    "required": [
      "name",
      "homepage"
    ]
  }
}
