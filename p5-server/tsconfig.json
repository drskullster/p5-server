{
  "compilerOptions": {
    "baseUrl": ".",
    "rootDir": "src",
    "outDir": "dist"
  },
  "extends": "../tsconfig.json",
  "exclude": [
    "dist",
    "build",
    "node_modules",
    "tests"
  ],
  "typedocOptions": {
    "name": "P5-server API documentation",
    "out": "build/docs",
    "excludePrivate": true,
    "excludeProtected": true,
    "readme": "./docs/api-index.md",
    "sort": [
      "static-first",
      "alphabetical"
    ]
  }
}
