{
  "compilerOptions": {
    "rootDir": "src",
    "outDir": "dist"
  },
  "exclude": ["build", "dist", "node_modules", "tests"],
  "extends": "..",
  "typedocOptions": {
    "name": "p5-analysis API documentation",
    "theme": "minimal",
    "out": "build/docs",
    "excludePrivate": true,
    "excludeProtected": true,
    "sort": ["static-first", "alphabetical"]
  }
}
